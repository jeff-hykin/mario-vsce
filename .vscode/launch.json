{
    "version": "0.2.0",
    "compounds": [
        
    ],
    "configurations": [
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": ["--extensionDevelopmentPath=${workspaceRoot}"],
            "stopOnEntry": false,
            "sourceMaps": true,
            "outFiles": ["${workspaceRoot}/**/*"],
            "preLaunchTask": "Run Package.json Fixer"
        }
    ]
}
