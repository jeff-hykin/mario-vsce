{
    "name": "mario",
    "displayName": "Mario: The Mario Extension",
    "description": "Cursor movement for speedrunners and pros",
    "version": "1.0.3",
    "publisher": "jeff-hykin",
    "repository": {
        "type": "git",
        "url": "https://github.com/jeff-hykin/mario-vsce.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "keywords": [
        "jumper",
        "jumpy",
        "cursor",
        "block",
        "jump",
        "navigate",
        "code"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:mario.moveUp",
        "onCommand:mario.moveDown",
        "onCommand:mario.moveToOuter",
        "onCommand:mario.moveDownToInner",
        "onCommand:mario.moveUpToInner"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "mario.moveUp",
                "title": "Mario: Move Up"
            },
            {
                "command": "mario.moveDown",
                "title": "Mario: Move Down"
            },
            {
                "command": "mario.moveToOuter",
                "title": "Mario: Move To Outer Block"
            },
            {
                "command": "mario.moveDownToInner",
                "title": "Mario: Move Down To Inner"
            },
            {
                "command": "mario.moveUpToInner",
                "title": "Mario: Move Up To Inner"
            }
        ]
    },
    "scripts": {
    },
    "devDependencies": {
        "vscode": "^1.0.0"
    }
}
