{
    "name": "mario",
    "displayName": "Mario: The Block Jumper Extension",
    "description": "Cursor movement for speedrunners and pros",
    "version": "2.0.3",
    "icon": "icon.png",
    "publisher": "jeff-hykin",
    "repository": {
        "type": "git",
        "url": "https://github.com/jeff-hykin/mario-vsce.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "keywords": [
        "jumper",
        "jumpy",
        "cursor",
        "block",
        "jump",
        "navigate",
        "code"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:mario.moveUp",
        "onCommand:mario.moveDown",
        "onCommand:mario.moveToOuter",
        "onCommand:mario.moveDownToInner",
        "onCommand:mario.moveUpToInner",
        "onCommand:mario.selectUp",
        "onCommand:mario.selectDown",
        "onCommand:mario.nextQuote",
        "onCommand:mario.previousQuote"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "mario.moveUp",
                "title": "Mario: Move Up"
            },
            {
                "command": "mario.moveDown",
                "title": "Mario: Move Down"
            },
            {
                "command": "mario.selectUp",
                "title": "Mario: Select Up"
            },
            {
                "command": "mario.selectDown",
                "title": "Mario: Select Down"
            },
            {
                "command": "mario.moveToOuter",
                "title": "Mario: Move To Outer Block"
            },
            {
                "command": "mario.selectToOuter",
                "title": "Mario: Select To Outer Block"
            },
            {
                "command": "mario.moveDownToInner",
                "title": "Mario: Move Down To Inner"
            },
            {
                "command": "mario.moveUpToInner",
                "title": "Mario: Move Up To Inner"
            },
            {
                "command": "mario.nextQuote",
                "title": "Mario: Move To Next Quote"
            },
            {
                "command": "mario.previousQuote",
                "title": "Mario: Move To Previous Quote"
            },
            {
                "command": "mario.selectNextQuote",
                "title": "Mario: Select To Next Quote"
            },
            {
                "command": "mario.selectPreviousQuote",
                "title": "Mario: Select To Previous Quote"
            },
            {
                "command": "mario.nextComma",
                "title": "Mario: Move To Next Comma"
            },
            {
                "command": "mario.previousComma",
                "title": "Mario: Move To Previous Comma"
            },
            {
                "command": "mario.selectNextComma",
                "title": "Mario: Select To Next Comma"
            },
            {
                "command": "mario.selectPreviousComma",
                "title": "Mario: Select To Previous Comma"
            }
        ]
    },
    "scripts": {
    },
    "devDependencies": {
        "vscode": "^1.0.0"
    }
}
